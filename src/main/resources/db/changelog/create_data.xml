<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="33d4e9bc-7210-47c3-9617-c56ab7c0750d" author="julian.b24" failOnError="true">

        <createTable tableName="data">
            <column name="id" type="varchar">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="age" type="numeric"/>
            <column name="income" type="numeric"/>
            <column name="city" type="varchar(255)"/>
            <column name="married" type="boolean"/>
            <column name="associate" type="boolean"/>
            <column name="expenses" type="number"/>
            <column name="account_type" type="varchar(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>